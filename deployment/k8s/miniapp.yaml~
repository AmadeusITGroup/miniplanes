apiVersion: apps/v1
kind: Deployment
metadata:
  name: itineraries-server-deployment
  labels:
    app: itineraries-server
spec:
  replicas: 1
  selector:
    matchLabels:
      app: itineraries-server
  template:
    metadata:
      labels:
        app: itineraries-server
    spec:
      containers:
      - name: itineraries-server
        image: itineraries-server:1.15.4
        ports:
        - containerPort: 80

---
